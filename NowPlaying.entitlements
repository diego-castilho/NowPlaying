<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <!-- ========================================= -->
        <!-- APP SANDBOX - HABILITADO (v0.9.3)        -->
        <!-- ========================================= -->
        <key>com.apple.security.app-sandbox</key>
        <true/>
        
        <!-- ========================================= -->
        <!-- NETWORK ACCESS                           -->
        <!-- ========================================= -->
        <!-- Necessário: Comunicação com Last.fm API -->
        <key>com.apple.security.network.client</key>
        <true/>
        
        <!-- ========================================= -->
        <!-- APPLE EVENTS                             -->
        <!-- ========================================= -->
        <!-- Necessário: Receber eventos do Apple Music -->
        <key>com.apple.security.automation.apple-events</key>
        <true/>
        
        <!-- ========================================= -->
        <!-- KEYCHAIN ACCESS GROUP                     -->
        <!-- ========================================= -->
        <!-- Permite acesso compartilhado ao Keychain -->
        <key>keychain-access-groups</key>
        <array>
            <string>$(AppIdentifierPrefix)com.diegocastilho.NowPlaying</string>
        </array>
        
        <!-- ========================================= -->
        <!-- APP IDENTIFIER (gerado automaticamente)  -->
        <!-- ========================================= -->
        <key>com.apple.application-identifier</key>
        <string>$(AppIdentifierPrefix)$(CFBundleIdentifier)</string>
        
        <!-- ========================================= -->
        <!-- TEAM IDENTIFIER (gerado automaticamente) -->
        <!-- ========================================= -->
        <key>com.apple.developer.team-identifier</key>
        <string>$(AppIdentifierPrefix)</string>
    </dict>
</plist>
